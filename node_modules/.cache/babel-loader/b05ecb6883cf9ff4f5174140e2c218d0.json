AI;AACP;AACAC,EAAAA,OAAO,CAACC,GAAR,CAAYF,KAAZ,EAFO,CAEY;;AACnB,SAAOG,OAAO,CAACC,MAAR,CAAeJ,KAAf,CAAP;AACD,CArBH,E,CAwBA;;AACApB,OAAO,CAACU,YAAR,CAAqBe,QAArB,CAA8Bb,GAA9B;AACE;AACF;AACA;AACA;;AAEE;AACF;AACA;AACA;AACA;AACE,UAAAa,QAAQ,EAAI;AACV,MAAMC,GAAG,GAAGD,QAAQ,CAACV,IAArB,CADU,CAGV;;AACA,MAAIW,GAAG,CAACC,IAAJ,KAAa,GAAjB,EAAsB;AACpB/B,IAAAA,OAAO,CAAC;AACNgC,MAAAA,OAAO,EAAEF,GAAG,CAACE,OAAJ,IAAe,OADlB;AAENC,MAAAA,IAAI,EAAE,OAFA;AAGNC,MAAAA,QAAQ,EAAE,IAAI;AAHR,KAAD,CAAP,CADoB,CAOpB;;AACA,QAAIJ,GAAG,CAACC,IAAJ,KAAa,KAAb,IAAsBD,GAAG,CAACC,IAAJ,KAAa,KAAnC,IAA4CD,GAAG,CAACC,IAAJ,KAAa,KAA7D,EAAoE;AAClE;AACAhC,MAAAA,UAAU,CAACoC,OAAX,CAAmB,gFAAnB,EAAqG,gBAArG,EAAuH;AACrHC,QAAAA,iBAAiB,EAAE,UADkG;AAErHC,QAAAA,gBAAgB,EAAE,QAFmG;AAGrHJ,QAAAA,IAAI,EAAE;AAH+G,OAAvH,EAIGK,IAJH,CAIQ,YAAM;AACZrC,QAAAA,KAAK,CAACsC,QAAN,CAAe,iBAAf,EAAkCD,IAAlC,CAAuC,YAAM;AAC3CE,UAAAA,QAAQ,CAACC,MAAT;AACD,SAFD;AAGD,OARD;AASD;;AACD,WAAOd,OAAO,CAACC,MAAR,CAAe,IAAIc,KAAJ,CAAUZ,GAAG,CAACE,OAAJ,IAAe,OAAzB,CAAf,CAAP;AACD,GArBD,MAqBO;AACL,WAAOF,GAAP;AACD;AACF,CAvCH,EAwCE,UAAAN,KAAK,EAAI;AACPC,EAAAA,OAAO,CAACC,GAAR,CAAY,QAAQF,KAApB,EADO,CACoB;;AAC3BxB,EAAAA,OAAO,CAAC;AACNgC,IAAAA,OAAO,EAAER,KAAK,CAACQ,OADT;AAENC,IAAAA,IAAI,EAAE,OAFA;AAGNC,IAAAA,QAAQ,EAAE,IAAI;AAHR,GAAD,CAAP;AAKA,SAAOP,OAAO,CAACC,MAAR,CAAeJ,KAAf,CAAP;AACD,CAhDH;AAmDA,eAAepB,OAAf","sourcesContent":["import axios from 'axios'\nimport { MessageBox, Message } from 'element-ui'\nimport store from '@/store'\nimport qs from \"qs\";\nimport { getToken } from '@/utils/auth'\n\n// create an axios instance\nconst service = axios.create({\n  baseURL: process.env.VUE_APP_BASE_API, // url = base url + request url\n  // withCredentials: true, // send cookies when cross-domain requests\n  timeout: 5000 // request timeout\n})\naxios.defaults.headers.post[\"Content-Type\"] = \"application/json\"; //配置请求\n// request interceptor\nservice.interceptors.request.use(\n  config => {\n    // do something before request is sent\n    if (config.method === \"post\") {\n      config.data = qs.parse(config.data);\n    } else {\n      config.params = config.data\n    }\n\n    if (store.getters.token) {\n      // let each request carry token\n      // ['X-Token'] is a custom headers key\n      // please modify it according to the actual situation\n      config.headers['X-Token'] = getToken()\n    }\n    return config\n  },\n  error => {\n    // do something with request error\n    console.log(error) // for debug\n    return Promise.reject(error)\n  }\n)\n\n// response interceptor\nservice.interceptors.response.use(\n  /**\n   * If you want to get http information such as headers or status\n   * Please return  response => response\n  */\n\n  /**\n   * Determine the request status by custom code\n   * Here is just an example\n   * You can also judge the status by HTTP Status Code\n   */\n  response => {\n    const res = response.data\n\n    // if the custom code is not 20000, it is judged as an error.\n    if (res.code !== '0') {\n      Message({\n        message: res.message || 'Error',\n        type: 'error',\n        duration: 5 * 1000\n      })\n\n      // 50008: Illegal token; 50012: Other clients logged in; 50014: Token expired;\n      if (res.code === 50008 || res.code === 50012 || res.code === 50014) {\n        // to re-login\n        MessageBox.confirm('You have been logged out, you can cancel to stay on this page, or log in again', 'Confirm logout', {\n          confirmButtonText: 'Re-Login',\n          cancelButtonText: 'Cancel',\n          type: 'warning'\n        }).then(() => {\n          store.dispatch('user/resetToken').then(() => {\n            location.reload()\n          })\n        })\n      }\n      return Promise.reject(new Error(res.message || 'Error'))\n    } else {\n      return res\n    }\n  },\n  error => {\n    console.log('err' + error) // for debug\n    Message({\n      message: error.message,\n      type: 'error',\n      duration: 5 * 1000\n    })\n    return Promise.reject(error)\n  }\n)\n\nexport default service\n"]}]}